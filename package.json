    "firebase": "8.2.3",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-hook-form": "^7.9.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-41.0.0.tar.gz",
    "react-native-dotenv": "^3.1.1",
    "react-native-draggable-flatlist": "^2.6.2",
